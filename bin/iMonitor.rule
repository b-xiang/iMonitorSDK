{
    "name" : "default",
    "description" : "default rule group",
    "rules" : [
        {
            "name" : "block notepad start",
            "action" : 1,
            "action_param" : "",
            "message" : ["ProcessCreate"],
            "matcher" : {
                "or" : [
                    {"match" : ["Path", "*\\notepad.exe"]},
                    {"match" : ["Path", "*\\notepad++.exe"]}
                ]
            }
        }
    ]
}
